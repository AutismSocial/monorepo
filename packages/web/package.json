{
  "name": "@autismjs/web",
  "version": "0.0.0",
  "description": "Web UI for Autism",
  "scripts": {
    "test:coverage": "NODE_ENV=development TS_NODE_PROJECT='./tsconfig.json' nyc --reporter=lcov --require ts-node/register tape './src/**/*.test.ts' | tap-spec; nyc report ---reporter=text",
    "test": "NODE_ENV=development TS_NODE_PROJECT='./tsconfig.json' tape -r ts-node/register './src/**/*.test.ts' | tap-spec",
    "build:web": "webpack --config webpack.config.js",
    "watch:web": "webpack --config webpack.config.js --watch",
    "build": "NODE_ENV=production concurrently --kill-others-on-fail npm:build:web",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js --hot",
    "eslint": "eslint . --fix",
    "tsc": "tsc --noEmit",
    "lint": "concurrently npm:tsc npm:eslint"
  },
  "author": "0xtsukino",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@autismjs/crypto": "^0.2.2",
    "@autismjs/message": "^0.2.1",
    "@chainsafe/libp2p-gossipsub": "^10.1.0",
    "@chainsafe/libp2p-noise": "^13.0.0",
    "@chainsafe/libp2p-yamux": "^5.0.0",
    "@libp2p/bootstrap": "^9.0.4",
    "@libp2p/kad-dht": "^10.0.4",
    "@libp2p/mdns": "^9.0.4",
    "@libp2p/mplex": "^9.0.4",
    "@libp2p/pubsub-peer-discovery": "^8.0.4",
    "@libp2p/tcp": "^8.0.5",
    "@libp2p/webrtc": "^3.1.7",
    "@libp2p/websockets": "^7.0.4",
    "async-mutex": "0.4.0",
    "commander": "^11.1.0",
    "dayjs": "^1.11.10",
    "eventemitter2": "^6.4.9",
    "it-all": "^3.0.3",
    "it-pipe": "^3.0.1",
    "libp2p": "^0.46.6",
    "node-cache": "^5.1.2",
    "uint8arrays": "^4.0.6"
  },
  "devDependencies": {
    "@libp2p/interface": "^0.1.2",
    "browserify-zlib": "^0.2.0",
    "constants-browserify": "^1.0.0",
    "console-browserify": "^1.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "crypto-browserify": "^3.12.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^8.1.0",
    "style-loader": "^3.3.3",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.6.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "randombytes": "^2.1.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "vm-browserify": "^1.1.2"
  }
}
