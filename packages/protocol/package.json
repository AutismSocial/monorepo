{
  "name": "@autismjs/protocol",
  "version": "0.2.5",
  "description": "Protocol implementation for Autism",
  "main": "build/index.js",
  "types": "build/src/index.d.ts",
  "browser": "build/browser.js",
  "scripts": {
    "test:coverage": "NODE_ENV=development TS_NODE_PROJECT='./tsconfig.json' nyc --reporter=lcov --require ts-node/register tape './src/**/*.test.ts' | tap-spec; nyc report ---reporter=text",
    "test": "NODE_ENV=development TS_NODE_PROJECT='./tsconfig.json' tape -r ts-node/register './src/**/*.test.ts' | tap-spec",
    "build:types": "tsc --project tsconfig.compile.json",
    "build:src": "webpack --config webpack.config.js",
    "build:web": "webpack --config webpack.browser.js",
    "build": "NODE_ENV=production concurrently --kill-others-on-fail npm:build:types npm:build:src npm:build:web",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js --hot",
    "eslint": "eslint . --fix",
    "tsc": "tsc --noEmit",
    "lint": "concurrently npm:tsc npm:eslint"
  },
  "files": [
    "build/",
    "src/",
    "readme.md"
  ],
  "author": "0xtsukino",
  "license": "MIT",
  "dependencies": {
    "@autismjs/crypto": "0.2.1",
    "@autismjs/db": "0.2.1",
    "@autismjs/message": "0.2.1",
    "@chainsafe/libp2p-gossipsub": "^10.1.0",
    "@chainsafe/libp2p-noise": "^13.0.0",
    "@chainsafe/libp2p-yamux": "^5.0.0",
    "@libp2p/bootstrap": "^9.0.4",
    "@libp2p/kad-dht": "^10.0.4",
    "@libp2p/mdns": "^9.0.4",
    "@libp2p/mplex": "^9.0.4",
    "@libp2p/pubsub-peer-discovery": "^8.0.4",
    "@libp2p/tcp": "^8.0.5",
    "@libp2p/webrtc": "^3.1.7",
    "@libp2p/websockets": "^7.0.4",
    "async-mutex": "0.4.0",
    "eventemitter2": "^6.4.9",
    "it-all": "^3.0.3",
    "it-pipe": "^3.0.1",
    "libp2p": "^0.46.6",
    "node-cache": "^5.1.2",
    "uint8arrays": "^4.0.6",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@libp2p/interface": "^0.1.2",
    "browserify-zlib": "^0.2.0",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "vm-browserify": "^1.1.2"
  }
}
